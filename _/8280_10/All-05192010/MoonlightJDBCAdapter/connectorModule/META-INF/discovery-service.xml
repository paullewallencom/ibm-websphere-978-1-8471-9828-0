<emd:discoveryService xmlns:emd="commonj.connector" xmlns:j2ee="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/j2ee_1_4.xsd">
	<j2ee:description>IBM WebSphere Adapter for JDBC</j2ee:description>
	<j2ee:display-name>IBM WebSphere Adapter for JDBC</j2ee:display-name>
	<emd:vendor-name xsi:type="j2ee:xsdStringType">IBM</emd:vendor-name>
	<emd:version xsi:type="j2ee:xsdStringType">7.0.0.0_IF02</emd:version>
	<emd:spec-version>1.2</emd:spec-version>
	<emd:discoveryService-class xsi:type="j2ee:fully-qualified-classType">
		com.ibm.j2ca.jdbc.emd.discovery.JDBCMetadataDiscovery
	</emd:discoveryService-class>
	<emd:metadataEdit-class xsi:type="j2ee:fully-qualified-classType">
		com.ibm.j2ca.jdbc.emd.discovery.JDBCMetadataEdit
	</emd:metadataEdit-class>
	<emd:adapterConfiguration-class xsi:type="j2ee:fully-qualified-classType">com.ibm.j2ca.jdbc.emd.discovery.JDBCAdapterConfiguration</emd:adapterConfiguration-class>
	<emd:application-specific-schema>
		<j2ee:description>JDBC ASI schema</j2ee:description>
		<j2ee:display-name>JDBC ASI schema</j2ee:display-name>
		<emd:asiNSURI>
			http://www.ibm.com/xmlns/prod/websphere/j2ca/jdbc/metadata
		</emd:asiNSURI>
		<emd:asiSchemaLocation>JDBCASI.xsd</emd:asiSchemaLocation>
	</emd:application-specific-schema>
</emd:discoveryService>